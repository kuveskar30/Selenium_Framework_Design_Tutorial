<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="2">
	<listeners>
		<listener
			class-name="seleniumFrameworkDesign.testComponents.TestNGListeners" />
	</listeners>
	<test thread-count="5" name="error validation tests">
		<classes>
			<class
				name="seleniumFrameworkDesign.testCases.ErrorValidations" />
		</classes>
	</test>
	<test name="Submit order tests">
		<classes>
			<class
				name="seleniumFrameworkDesign.testCases.ModifiedStandaloneTest" />
		</classes>
	</test> <!-- Test -->

	
</suite> <!-- Suite -->
